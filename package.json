		"babel-preset-env": "^1.7.0",
		"babel-preset-stage-2": "^6.24.1",
		"backpack-core": "^0.8.4",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-prettier": "^3.1.2",
		"expose-loader": "^0.7.5",
		"flow-bin": "^0.121.0",
		"gulp": "^4.0.2",
		"gulp-add-src": "^1.0.0",
		"gulp-babel": "^8.0.0",
