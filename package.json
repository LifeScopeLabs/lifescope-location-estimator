		"gulp-uglify": "^3.0.2",
		"gulp-util": "^3.0.8",
		"gulp-zip": "^5.0.1",
		"jest": "^25.1.0",
		"mongodb-memory-server": "^6.4.0",
		"nodemon": "^2.0.2",
